module codegen.operators.Equals

enum Bool
  False : Bool
  True : Bool


record Equals a
  equals : a -> a -> Bool


equals : a => implicit Equals a -> a -> a -> Bool
equals ev x y =
  ev.equals x y


boolEquals : Bool -> Bool -> Bool
boolEquals x y = 
  False


implicit
boolEqualsInstance : Equals Bool
boolEqualsInstance =
  Equals boolEquals


(+) : Int -> Int -> Int
(+) x y =
  0
  