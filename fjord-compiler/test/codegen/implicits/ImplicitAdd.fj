module codegen.implicits.ImplicitAdd

import TestUtil

record Add a
  add : a -> a -> a

addInt : Int -> Int -> Int
addInt x y =
  rm x (rm y 0)


addString : String -> String -> String
addString x y =
  rm x (rm y "")


implicit
intInstance : Add Int
intInstance =
  Add addInt

implicit
stringInstance : Add String
stringInstance =
  Add addString

(++) : a. Add a => a -> a -> a
(++) ev x y =
  ev.add x y

three : Int
three = 
  1 ++ "no"

helloWorld : String
helloWorld =
  "hello, " ++ 
 "world"
