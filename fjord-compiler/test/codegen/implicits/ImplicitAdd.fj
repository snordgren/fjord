module codegen.implicits.ImplicitAdd

record Add a
  add : a -> a -> a

addInt : Int -> Int -> Int
addInt x y =
  0


addString : String -> String -> String
addString x y =
  ""


implicit
intInstance : Add Int
intInstance =
  Add addInt

implicit
stringInstance : Add String
stringInstance =
  Add addString

(++) : a => implicit Add a -> a -> a -> a
(++) ev x y =
  ev.add x y

three : Int
three = 
  1 ++ 2

helloWorld : String
helloWorld =
  "hello, " ++ 
 "world"
